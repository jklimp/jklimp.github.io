    var buttonSchusterick = document.getElementById('buttonSchusterick');
    function drawChart() {
       // Disabling the button while the chart is drawing.
      buttonDefault.disabled = true;
      buttonGibson.disabled = true;
      buttonMcMahon.disabled = true;
      buttonKoling.disabled = true;
      buttonDoss.disabled = true;
      buttonSexton.disabled = true;
      buttonLocastro.disabled = true;
      buttonMcBeth.disabled = true;
      buttonUlibarri.disabled = true;
      buttonBrathwaite.disabled = true;
      buttonWysocki.disabled = true;
      buttonLizotte.disabled = true;
      buttonSchusterick.disabled = true;
      google.visualization.events.addListener(chart, 'ready',
          function() {
            buttonDefault.disabled = false;
            buttonGibson.disabled = false;
            buttonMcMahon.disabled = false;
            buttonKoling.disabled = false;
            buttonDoss.disabled = false;
            buttonSexton.disabled = false;
            buttonLocastro.disabled = false;
            buttonMcBeth.disabled = false;
            buttonUlibarri.disabled = false;
            buttonBrathwaite.disabled = false;
      			buttonWysocki.disabled = false;
      			buttonLizotte.disabled = false;
      			buttonSchusterick.disabled = false;
					});
      chart.draw(data[current], options);
    }
    drawChart();
    buttonDefault.onclick = function() {
      current = 0;
      drawChart();
    }
    
    buttonGibson.onclick = function() {
      current = 1;
      drawChart();
    }
    
    buttonMcMahon.onclick = function() {
      current = 2;
      drawChart();
    }
    buttonKoling.onclick = function() {
      current = 3;
